{
  "name": "gullak",
  "version": "1.0.0",
  "description": "A personal finance and wishlist app",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "server": "nodemon server.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\" --ignore client",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client"
  },
  "keywords": [
    "gullak"
  ],
  "author": "Talal Najam",
  "license": "MIT",
  "dependencies": {
    "async": "^3.1.0",
    "bcryptjs": "^2.4.3",
    "concurrently": "^4.1.1",
    "config": "^3.2.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.7.5",
    "nodemailer": "^6.3.1",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "validator": "^13.7.0"
  },
  "devDependencies": {
    "nodemon": "^1.19.1"
  }
}
